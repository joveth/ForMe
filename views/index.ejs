  <script  type="text/javascript">
	$(document).ready(function(){
		var mainmen = $("#mainmenu");
		var defaultHeight = mainmen.height();
		var flag = true;
		$("#menu_icon").click(function(){
			if(flag){
				mainmen.animate({height:'300px'},400);
				mainmen.animate({height:'150px'},400);
				mainmen.animate({height:'60px'},400,function(){
					mainmen.children("div").hide();
				});
			}else{
				mainmen.animate({height:'80px'},100,function(){
					mainmen.children("div").show();
				});
				mainmen.animate({height:'150px'},400);
				mainmen.animate({height:defaultHeight},400);
			}
			flag = !flag;
			return false;
		});
	});
</script>
